{
    "manifest_version": 2,
    "name": "memo - 刷个课",
    "version": "1.0.2",
    "description": "iRead 发现",
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "minimum_chrome_version": "6.0.0.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "64": "icon.png"
    },
    "content_scripts": [{
        "matches": ["http://202.115.133.161/*"],
        "js": ["jquery-1.7.1.js", "contentScript.js"],
        "css": ["style.css"]
    }]
}
